<%= form_with(model: provider, local: true) do |form| %>
  <div class="field">
    <%= form.label :address %>
    <%= form.text_field :address %>
  </div>

  <div class="field">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div class="field">
    <%= form.label :phone_number %>
    <%= form.telephone_field :phone_number %>
  </div>

  <div class="field">
    <%= form.label :primary_contact %>
    <%= form.text_field :primary_contact %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
